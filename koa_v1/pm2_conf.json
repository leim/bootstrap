{
  "apps": [
    {
      "name": "app",
      "cwd": "./",
      "script": "app.js",
      "watch": true,
      "ignore_watch": [
        "public"
      ],
      "env": {
        "NODE_ENV": "test"
      },
      "merge_logs": true,
      "error_file": "./public/console.log",
      "out_file": "./public/console.log",
      "instance": 2,
      "max_memory_restart": "1G",
      "max_restarts": 0,
      "exec_mode": "cluster",
      "autorestart": true,
      "exec_interpreter": "node"
    }
  ]
}